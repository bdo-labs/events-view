{
  "name": "events-view",
  "version": "0.0.0",
  "description": "Component that lists events.",
  "main": "lib/index.js",
  "style": "lib/main.css",
  "repository": "git://github.com/bdo-labs/events-view.git",
  "license": "MIT",
  "scripts": {
    "test": "karma start"
  },
  "atomify": {
    "js": {
      "entry": "lib/index.js",
      "output": "build/build.js"
    },
	"css": {
		"entry": "index.css",
		"output": "build/bundle.css",
		"assets": {
			"dest": "build/assets/",
			"prefix": "assets/"
		}
	}
  },
  "browserify": {
    "transform": [
      "partialify"
    ]
  },
  "devDependencies": {
    "angular": "^1.2.18",
    "atomify": "^3.1.2",
    "partialify": "^3.1.1",
    "browserify": "^4.2.0",
    "karma": "^0.12.16",
    "karma-browserifast": "^0.6.1",
    "karma-jasmine": "^0.1.5"
  },
  "dependencies": {
    "filters": "git://github.com/bdo-labs/filters.git",
    "styles": "git://github.com/bdo-labs/styles.git",
    "services": "git+https://edbdo.visualstudio.com/DefaultCollection/BDO%20Labs/_git/Services"
  }
}
